{
    "name": "nako-color-theme",
    "displayName": "Nako",
    "description": "VS Code color theme inspired by Vercel syntax highlighting",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/kettanaito/vscode-nako-theme"
    },
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Nako",
                "uiTheme": "vs-dark",
                "path": "./themes/nako-dark-theme.json"
            },
            {
                "label": "Nako (Light)",
                "uiTheme": "vs",
                "path": "./themes/nako-light-theme.json"
            }
        ]
    }
}